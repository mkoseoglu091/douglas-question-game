-- magic 8 ball game

-- init
function init_magic8()
 q = "ask me a \nyes or no \nquestion!"
 a = {"yes, \ndefinitely!!", "without a \ndoubt!!", "signs point \nto yes...", "most \nlikely...", "as i \nsee it, \nyes...", "very \ndoubtful...", "my sources \nsay no.", "my reply \nis no.", "don't count \non it...", "signs point \nto no...", "cannot \npredict \nnow...", "concentrate \nand ask \nagain.", "ask again \nlater.", "better not \ntell you \nnow...", "\ntry again."}
 ans = ""
 state = 0; -- 0 = ask, 1 = think, 2 = answer
 wait_period = 0
 wait_limit = 50
 timer_limit = 35
 bubble_y = 40
 first = true
 timer = 0
 music()
end

-- update
function update_magic8()
 if (state == 0) then
 	if(first) then
 	 sfx(4)
 	 first=false
 	end 
  if btnp(❎) and (timer > timer_limit) then 
   timer = 0
   state=1 
  end
  timer += 1

 elseif (state == 1) then
  if (wait_period == 0) then
  sfx(5)
  first = true
  num = 1 + flr(rnd(count(a)))
  ans = a[num]
  elseif (wait_period > wait_limit) then 
   state=2 
  end
  wait_period = wait_period + 1
 else -- state 2
  if first then
   sfx(6)
   first = false
  end
  if btnp(❎) and timer > timer_limit then 
   wait_period = 0
   first = true
   timer = 0
   state=0 
  end
  timer += 1
 end 
end

-- draw
function draw_magic8()
 cls(3) --clear screen
 spr(1, 2, 4, 8, 2) -- douglas
 spr(32, 68, 4, 8, 2) -- college
 spr(128, 4, 30, 8, 8) -- roary
 -- speech bubbles
 if (state==0) then 
  spr(64, 62, bubble_y + 2, 8, 4)
  print(q, 76, bubble_y + 12, 1)
  print("to continue press ❎", 22, 110, 7)
 elseif (state==1) then
  spr(64, 62, bubble_y + 2, 8, 4)
  print("\nhmmm...",78, bubble_y + 12, 1)
 elseif (state==2) then
  spr(64, 62, bubble_y + 2, 8, 4) 
  print(ans, 76, bubble_y + 12, 1)
  print("to return press ❎", 22, 110, 7)
 end
 
 -- expressions
 if (state == 0) then
  if timer<8 then
   spr(204, 20, 46, 4, 4)
  elseif timer<16 then
   spr(200, 20, 46, 4, 4) 
  elseif timer<24 then
   spr(204, 20, 46, 4, 4) 
  else
   spr(200, 20, 46, 4, 4) 
  end
 elseif (state == 1) then
  spr(140, 20, 46, 4, 4)
	elseif (state==2) then
	 -- answering question
	 if num<=5 then
	  if timer<8 then
    spr(204, 20, 46, 4, 4)
   elseif timer<16 then
    spr(200, 20, 46, 4, 4) 
   elseif timer<24 then
    spr(204, 20, 46, 4, 4) 
   else
    spr(200, 20, 46, 4, 4) 
   end
	 elseif (num <= 10 and num > 5) then
	 	spr(136, 20, 46, 4, 4)
	 elseif (num > 10) then
	  if timer<5 then
	   spr(74, 12, 42, 2, 2)
	  elseif timer<10 then
	   spr(74, 12, 44, 2, 2)	  
	  elseif timer<15 then
	   spr(74, 12, 46, 2, 2)
	  elseif timer<20 then
	   spr(74, 12, 48, 2, 2)
	  else
	   spr(74, 12, 50, 2, 2) 
	  end 
	 end
	end 
end

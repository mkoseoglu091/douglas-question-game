-- trivia game

-- init
function init_trivia()
 choice = 1 -- can be 1,2,3,4
 highlight_x = 26
 highlight_y = 100
 score = 0 -- every correct answer increases score by 1
end

-- update
function update_trivia()
 if btnp(➡️) then
  if choice < 4 then choice +=1
  else choice = 1 end
 elseif btnp(⬅️) then
  if choice > 1 then choice -=1
  else choice = 4 end
 end
end

-- draw
function draw_trivia()
 cls(3) --clear screen
 spr(1, 2, 4, 8, 2) -- douglas
 spr(32, 68, 4, 8, 2) -- college
 spr(128, 4, 30, 8, 8) -- roary
 -- speech bubble
 
 -- buttons
 if choice == 1  then
  highlight_x = 26
 elseif choice == 2 then
  highlight_x = 46
 elseif choice == 3 then
  highlight_x = 66
 elseif choice == 4 then
  highlight_x = 86
 end 
 -- normal buttons
 spr(10, 26, 100, 2, 2)
 spr(10, 46, 100, 2, 2)
 spr(10, 66, 100, 2, 2)
 spr(10, 86, 100, 2, 2)
 -- highlight  button
 spr(42, highlight_x, highlight_y, 1, 2)
 spr(45, highlight_x+8, highlight_y, 1, 2)
 -- button labels
 print("a", 33, 102, 7)
 print("b", 53, 102, 7)
 print("c", 73, 102, 7)
 print("d", 93, 102, 7)
end

pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
-- main functions

-- init
function _init()
 game_mode = 0 -- 0=selection, 1=magic8, 2=trivia
 game_selection = 1
end

-- update
function _update()
 if game_mode == 0 then
  update_main()
 elseif game_mode == 1 then
  update_magic8()
 elseif game_mode == 2 then
  update_trivia()
 end
end

--draw
function _draw()
 if game_mode == 0 then
  draw_main()
 elseif game_mode == 1 then
  draw_magic8()
 elseif game_mode == 2 then
  draw_trivia()
 end
end


-- main screen
function update_main()
 if btnp(⬅️) or btnp (➡️) then
  if game_selection == 1 then game_selection=2
  elseif game_selection == 2 then game_selection=1 end
 end
 if btnp(❎) then
  if game_selection == 1 then
   init_magic8()
   game_mode=1
  elseif game_selection == 2 then
   init_trivia()
   game_mode=2
  end
 end
end

function draw_main()
	cls()
	cls(3) --clear screen
 spr(1, 2, 4, 8, 2) -- douglas
 spr(32, 68, 4, 8, 2) -- college
 spr(128, 32, 30, 8, 8) -- roary

 -- buttons
 if game_selection == 1 then
  spr(42, 30, 100, 4, 2)
  spr(10, 66, 100, 4, 2)
 elseif game_selection == 2 then
  spr(10, 30, 100, 4, 2)
  spr(42, 66, 100, 4, 2)
 end
 print("fortune", 32, 102, 7)
 print("triia", 72, 102, 7)
 print("press ❎ to select", 29, 112)
end
